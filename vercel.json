{
    "version": 2,
    "scope": "polart",
    "github": {
        "enabled": true
    },
    "routes": [
        {
            "src": "/react-spreadsheet",
            "dest": "index.html"
        },
        {
            "src": "/react-spreadsheet/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/react-spreadsheet/(.*)",
            "dest": "$1"
        }
    ]
}
